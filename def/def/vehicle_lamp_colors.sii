	color[]: (1.00, 1.00, 1.00)	# 4. high beam
	color[]: (1.00, 0.31, 0.00)	# 5. front turn signal
	color[]: (1.00, 0.31, 0.00)	# 6. rear turn signal
	color[]: (1.00, 0.05, 0.00)	# 7. brake
	color[]: (1.00, 1.00, 1.00)	# 8. reverse
	color[]: (1.00, 1.00, 1.00)	# 9. daytime running
