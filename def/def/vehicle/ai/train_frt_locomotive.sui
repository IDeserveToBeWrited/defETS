#----------------------------------------------------------------
# switzerland
#----------------------------------------------------------------
traffic_vehicle : traffic.frt.ch
{
	accessories[]: .frt.ch.chassis
	accessories[]: .frt.ch.fwheel
	accessories[]: .frt.ch.rwheel

	max_speed: 160

	vehicle_mass: 132000
	engine_power: 2000

	trailer_chains[]: "train_car.frt.ch.car1 | train_car.frt.ch.car2 | train_car.frt.ch.car_last"

	sound_move[]: "/sound/traffic/train_electric_1.soundref"
	sound_gears: 1

	displacement_limit: 0.0

	spawn_ratio: 0.0

	tags[]: passenger

	lod_dist[]: 70
	lod_dist[]: 130

}

vehicle_accessory: .frt.ch.chassis
{
	data_path: "/def/vehicle/ai/train_frt/chassis_loco_che.sii"
}

vehicle_wheel_accessory: .frt.ch.fwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/front_empty_50cm.sii"
}

vehicle_wheel_accessory: .frt.ch.rwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/rear_empty_50cm.sii"
}

