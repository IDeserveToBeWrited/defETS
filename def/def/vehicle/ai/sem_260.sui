traffic_vehicle : traffic.sem_crane
{
	accessories[]: .sem_crane.chassis
	accessories[]: .sem_crane.fwheel
	accessories[]: .sem_crane.rwheel
	accessories[]: .sem_crane.rwheel2

	max_speed: 70

	vehicle_mass: 10000

	sound_move[]: "/sound/traffic/truck_eu_2.soundref"
	sound_horn[]: "/sound/traffic/truck_horn_1.soundref"

	

	color[]: (0.7341, 0.1253, 0.01) #orange
	color[]: (0.7454, 0.7454, 0.7454) #white
	color[]: (0.0273, 0.0998, 0.1811) #blue

	allow_parked: false

	spawn_ratio: 0.3

@include "drivers_generic.sui"
}

vehicle_accessory: .sem_crane.chassis
{
	data_path: "/def/vehicle/ai/sem_260/crane.sii"
}

vehicle_wheel_accessory: .sem_crane.fwheel
{
	offset: 0
	data_path: "/def/vehicle/f_wheel/steel.sii"
}

vehicle_wheel_accessory: .sem_crane.rwheel
{
	offset: 0
	data_path: "/def/vehicle/r_wheel/steel.sii"
}

vehicle_wheel_accessory: .sem_crane.rwheel2
{
	offset: 2
	data_path: "/def/vehicle/r_wheel/steel.sii"
}


