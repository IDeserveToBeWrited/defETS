traffic_trailer : traffic.trailer.aero_dynamic
{
	accessories[]: .traffic.aero_dynamic.tchassis
	accessories[]: .traffic.aero_dynamic.trwheel0
	accessories[]: .traffic.aero_dynamic.trwheel1
	accessories[]: .traffic.aero_dynamic.trwheel2
	cargo_mass: 22000
}

vehicle_accessory: .traffic.aero_dynamic.tchassis
