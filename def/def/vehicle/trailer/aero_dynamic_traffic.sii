    	data_path: "/def/vehicle/trailer/aero_dynamic/chassis.sii"
}

vehicle_wheel_accessory: .traffic.aero_dynamic.trwheel0
{
	offset: 0
    	data_path: "/def/vehicle/t_wheel/single.sii"
}

vehicle_wheel_accessory: .traffic.aero_dynamic.trwheel1
{
	offset: 2
    	data_path: "/def/vehicle/t_wheel/single.sii"
}

vehicle_wheel_accessory: .traffic.aero_dynamic.trwheel2
{
	offset: 4
    	data_path: "/def/vehicle/t_wheel/single.sii"
}

