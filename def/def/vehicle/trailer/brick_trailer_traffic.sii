traffic_trailer : traffic.trailer.brk_trailer
{
	accessories[]: .traffic.brk_trailer.tchassis
	accessories[]: .traffic.brk_trailer.trwheel0
	accessories[]: .traffic.brk_trailer.trwheel1
	accessories[]: .traffic.brk_trailer.trwheel2
	cargo_mass: 23000
}

vehicle_accessory: .traffic.brk_trailer.tchassis
{
    	data_path: "/def/vehicle/trailer/brick_trailer/chassis.sii"
}

vehicle_wheel_accessory: .traffic.brk_trailer.trwheel0
{
	offset: 0
    	data_path: "/def/vehicle/t_wheel/single.sii"
}

vehicle_wheel_accessory: .traffic.brk_trailer.trwheel1
{
	offset: 2
    	data_path: "/def/vehicle/t_wheel/single.sii"
}

vehicle_wheel_accessory: .traffic.brk_trailer.trwheel2
{
	offset: 4
    	data_path: "/def/vehicle/t_wheel/single.sii"
}

