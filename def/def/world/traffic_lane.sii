#
# NOTE: The first traffic lane definition is used as default if not set otherwise (e.g. for unconnected prefabs or trajectories)

traffic_lane_data : traffic_lane.default
{
	speed_class: local_road

	rank: 0

	traffic_rules[]: traffic_rule.road_rail
}

traffic_lane_data : traffic_lane.road.local
{
	# Traffic lane with 'local' speed limits and low spawn density
	# Allows changing lanes (when used for multi-lane roads), but NOT overtaking in opposite lane

	speed_class: local_road

	rank: 50

	traffic_rules[]: traffic_rule.road
	traffic_rules[]: traffic_rule.s_car_l
	traffic_rules[]: traffic_rule.s_trl_r
